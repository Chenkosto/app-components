{
  "name": "@datorama/app-components",
  "version": "0.1.5",
  "description": "Datorama React components library",
  "author": "Shay Keinan",
  "repository": "https://github.com/datorama/app-components",
  "homepage": "https://app-components.herokuapp.com",
  "license": "MIT",
  "peerDependencies": {
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/lodash": "^4.14.126",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "^4.1.14",
    "autoprefixer": "^9.4.9",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^1.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "replace": "^1.1.0",
    "rimraf": "2.6.2",
    "style-loader": "^0.23.1",
    "styled-components": "4.1.1",
    "terser-webpack-plugin": "1.1.0",
    "typescript": "^3.4.5",
    "webpack": "4.29.6",
    "webpack-cli": "^3.2.2"
  },
  "scripts": {
    "heroku-prebuild": "cd app && npm install",
    "start": "cd app && npm start",
    "heroku-postbuild": "cd app && npm run build",
    "lib:build": "rimraf dist && webpack && cd app && npm run generate:docs",
    "lib:watch": "rimraf dist && webpack -w",
    "lib:version": "replace '<Version>.+<\/Version>' '<Version>'$npm_package_version'</Version>' ./app/src/App.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "app/src/**/*.{js,jsx,tsx,json,css,scss}": [
        "prettier --single-quote --write",
        "git add"
      ]
    }
  }
}
